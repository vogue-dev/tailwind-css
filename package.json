{
    "name": "some",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@testing-library/jest-dom": "^5.11.6",
        "@testing-library/react": "^11.2.2",
        "@testing-library/user-event": "^12.2.2",
        "autoprefixer": "^10.0.4",
        "postcss": "^8.1.10",
        "postcss-cli": "^8.3.0",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-grid-layout": "^1.2.0",
        "react-scripts": "4.0.1",
        "tailwindcss": "^2.0.1",
        "web-vitals": "^0.2.4"
    },
    "scripts": {
        "start": "npm run build:css & react-scripts start",
        "build": "npm run build:watch && react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "build:css": "postcss src/styles/tailwind.css -o src/styles/main.css",
        "build:watch": "postcss src/styles/tailwind.css -o src/styles/main.css -w"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
